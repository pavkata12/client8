{
  "server": {
    "host": "192.168.7.3",
    "port": 8080,
    "websocket_endpoint": "/ws",
    "reconnect_interval": 5,
    "max_reconnect_attempts": 10,
    "fallback_hosts": ["127.0.0.1", "localhost", "192.168.0.100"]
  },
  "client": {
    "auto_start": true,
    "auto_reconnect": true,
    "max_reconnect_attempts": 10,
    "debug": true,
    "minimize_to_tray": true,
    "show_timer_overlay": true,
    "show_security_widget": true,
    "timer_position": {
      "x": 200,
      "y": 40
    },
    "security_widget_position": {
      "x": 620,
      "y": 40
    }
  },
  "ui": {
    "timer_position": {
      "x": 200,
      "y": 40
    },
    "notifications": true,
    "gaming_theme": true,
    "timer_font_size": 48,
    "status_font_size": 14,
    "timer_opacity": 0.9,
    "theme_colors": {
      "primary": "#00FF88",
      "secondary": "#FF6B6B",
      "warning": "#FFD700",
      "danger": "#FF4444",
      "background": "#1a1a2e"
    }
  },
  "security": {
    "enhanced_protection": true,
    "process_monitoring": true,
    "system_restrictions": true,
    "network_filtering": true,
    "keyboard_blocking": {
      "enabled": true,
      "block_windows_key": true,
      "block_ctrl_esc": true,
      "block_alt_f4": true,
      "block_alt_tab": true,
      "block_ctrl_shift_esc": true,
      "block_function_keys": true,
      "block_print_screen": true
    },
    "process_restrictions": {
      "block_task_manager": true,
      "block_registry_editor": true,
      "block_command_prompt": true,
      "block_powershell": true,
      "block_control_panel": true,
      "block_dev_tools": true,
      "block_file_explorer": false,
      "block_browsers_admin": false
    },
    "system_restrictions": {
      "disable_task_manager": true,
      "disable_registry_editor": true,
      "disable_command_prompt": true,
      "disable_control_panel": true,
      "disable_system_restore": true,
      "restrict_uac_changes": true
    },
    "network_restrictions": {
      "block_dangerous_ports": true,
      "blocked_ports": [22, 23, 3389, 5900, 5901],
      "domain_filtering": true,
      "blocked_domains": [
        "malware.com",
        "virus.com",
        "hacking.com",
        "cracking.com"
      ]
    },
    "admin_password": "admin123",
    "security_alerts": {
      "show_process_warnings": true,
      "show_keyboard_blocks": false,
      "show_network_blocks": true,
      "alert_duration": 5000
    }
  },
  "logging": {
    "level": "INFO",
    "file": "netcafe_client_enhanced.log",
    "max_size": "10MB",
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "performance": {
    "security_check_interval": 2,
    "status_update_interval": 5,
    "timer_update_interval": 1000,
    "memory_cleanup_interval": 300
  }
} 